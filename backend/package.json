{
    "name": "uol-backend",
    "version": "1.0.0",
    "description": "app para gerenciamento de cadastro de clientes",
    "main": "index.js",
    "scripts": {
        "db:reset": "npx prisma migrate reset",
        "db:init": "npx prisma migrate dev --name init",
        "build": "tsc --project tsconfig.json",
        "prestart": "npm run build && npm run db:reset",
        "start": "node ./build/server.js",
        "predev": "npm run db:init",
        "dev": "tsnd --watch \"./src/**\" --transpile-only ./src/server.ts",
        "generate-schema": "npx prisma generate",
        "test": "jest"
    },
    "keywords": [],
    "author": "vania ioott",
    "license": "ISC",
    "devDependencies": {
        "@types/cors": "2.8.12",
        "@types/express": "4.17.13",
        "@types/node": "16.11.7",
        "@types/validator": "13.6.6",
        "@typescript-eslint/eslint-plugin": "^6.2.0",
        "@typescript-eslint/parser": "^6.2.0",
        "@types/jest": "^29.5.12",
        "jest": "^27.5.0",
        "ts-node": "^10.9.2",
        "ts-node-dev": "^2.0.0",
        "typescript": "^5.3.3"
    },
    "dependencies": {
        "@prisma/client": "^5.9.1",
        "cors": "^2.8.5",
        "dotenv": "^16.4.4",
        "express": "^4.18.3",
        "helmet": "^7.1.0",
        "ts-loader": "^9.5.1",
        "joi": "^17.12.2"
    }
}
